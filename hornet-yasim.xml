<!--   -->

<airplane mass="416"> <!-- empty (245) + pilot (225) - rotorblades (54)-->

	<approach speed="40" aoa="16">
		<control-setting axis="/controls/engines/engine[0]/throttle" value="0.3"/>
	</approach>

	<cruise speed="84" alt="0">
		<control-setting axis="/controls/engines/engine[0]/throttle" value="1.0"/>
	</cruise>





	<rotor name="main" x="-1.68" y="0.0" z="1.12" nx="-0.12" ny="0" nz="1." fx="1" fy="0" fz="0" ccw="1"
	  maxcollective="2.0" mincollective="2.0"
		diameter="7"
		numblades="2"
		weightperblade="30"
		twist="-0"
		chord="0.2"
	  rpm="350"
		tiltcenterx="-1.655" tiltcentery="0" tiltcenterz="0.99"
	  mintiltpitch=" -7" 
	  maxtiltpitch="13"
	  mintiltroll="-11" 
	  maxtiltroll=" 11" 
	  relbladecenter="0.5"
	  dynamic="1"
	  sharedflaphinge="1"
	  rellenflaphinge="0.00"
	  delta3="0"
	  delta=".8"
	  pitch-a="10"
	  pitch-b="15"
	  flapmin="-10"
	  flapmax="10"
	  flap0="-1.5"
	  flap0factor="0"
	  notorque="0"
	  dragfactor="0.30"
	  translift-ve="20"
	  translift-maxfactor="1.3"
	  ground-effect-constant="0.1"
	  taper="1"
	  number-of-segments="8"
	  number-of-parts="8"
	  rel-len-where-incidence-is-measured="0.7"
	  rel-len-blade-start="0.13"

	  airfoil-lift-coefficient="5.7"
	  airfoil-drag-coefficient0="0.01"
	  airfoil-drag-coefficient1="0.028"
	  airfoil-incidence-no-lift="-1,5"
	  incidence-stall-zero-speed="8"
	  incidence-stall-half-sonic-speed="7"
	  lift-factor-stall="0.2"
	  stall-change-over="10"
	  drag-factor-stall="8.0"
	  cyclic-factor="0.8"
	  rotor-correction-factor="0.5"
	  balance="0.99985"

>
		<control-input axis="/controls/flight/aileron-trim" control="TILTROLL" split="true"
					   src0="-1.0" src1="1.0"
					   dst0="1.0" dst1="-1.0"/>
		<control-input axis="/controls/flight/aileron" control="TILTROLL"
					   src0="-1.0" src1="1.0"
					   dst0="1.0" dst1="-1.0"/>
		<control-input axis="/controls/flight/elevator-trim" control="TILTPITCH" split="true"
					   src0="-1.0" src1="1.0"
					   dst0="1.0" dst1="-1.0"/>
		<control-input axis="/controls/flight/elevator" control="TILTPITCH"
					   src0="-1.0" src1="1.0"
					   dst0="1.0" dst1="-1.0"/>
	</rotor>





	<rotorgear
	  max-power-engine="2"
	  rotorgear-friction="0.1"
	  engine-prop-factor="0.1"
	  engine-accel-limit="16"
	  yasimdragfactor="10"
	  yasimliftfactor="300"
>
		<control-input axis="/controls/rotor/handstart" control="ROTORGEARENGINEON"/>
		<control-input axis="/controls/rotor/reltarget" control="ROTORRELTARGET"
					   src0="0.0" src1="1.0"
					   dst0="0.0" dst1="1.0"/>
	</rotorgear>

	<propeller radius="0.810"
	   cruise-speed="80" cruise-rpm="2900"
           cruise-alt="8000" cruise-power="25"
           takeoff-power="30" takeoff-rpm="3035"
           gear-ratio="0.39" 
           x="-1.95" y="0" z="0.07" mass="88" moment="0.1">
		<piston-engine
               eng-power="39"
               eng-rpm="6800"
               displacement="26.64"
               compression="6.3"
		/>
		<actionpt x="-2.28" y="0" z="-0.09"/>
		<control-input axis="/controls/engines/engine[0]/throttle" control="THROTTLE"/>
		<control-input axis="/controls/engines/engine[0]/starter" control="STARTER"/>
		<control-input axis="/controls/engines/engine[0]/magnetos" control="MAGNETOS"/>
		<control-input axis="/controls/engines/engine[0]/mixture" control="MIXTURE"/>
	</propeller>




	<cockpit x="-1.579" y="0" z="0.315"/>

	<fuselage ax="-0.5" ay="0" az="-0.16" bx="-1.63" by="0" bz="-0.16"
			  width="0.65" taper="0.5" midpoint="0.5"/>



	<vstab x="-3" y="0" z="-0.94" taper=".5" length="0.9" chord="1.1" sweep="28" incidence="0">
		<stall aoa="16" width="4" peak="1.5"/>
		<flap0 start="0" end="1" lift="1.3" drag="1.2"/>
		<control-input axis="/controls/flight/rudder" control="FLAP0" invert="true"/>
		<control-input axis="/controls/flight/rudder-trim" control="FLAP0" invert="true"/>
		<control-output control="FLAP0" prop="/surface-positions/rudder-pos-norm"
				min="1" max="-1"/>

	</vstab>


	<hstab x="-3" y="0.0" z="-0.81" taper="0.5" effectiveness="1.0" length="1.29" chord="0.84" sweep="28" incidence="-3">
		<stall aoa="0" width="0" peak="1.5"/>
	</hstab>

	<!-- noze gear 1.624-->
	<gear x="-.33" y="0.000" z="-1.220" compression="0.1" spring="1.5" damp="0.75" sfric=".8" dfric=".6">
                <control-input axis="/controls/flight/rudder" control="STEER" square="false"
					   src0="-1.0" src1="1.0"
					   dst0="-0.2" dst1="0.2"/>

		<control-input axis="/controls/gear/brake-left" control="BRAKE" split="true"/>
		<control-input axis="/controls/gear/brake-parking" control="BRAKE" split="true"/>
	</gear>

	<!-- left gear .-->
	<gear x="-1.64" y="0.879" z="-1.254" compression="0.1" spring="3.5" damp="0.75" sfric=".5" dfric=".4" initial-load="0.8">
		<control-input axis="/controls/gear/brake-left" control="BRAKE" split="true"/>
		<control-input axis="/controls/gear/brake-parking" control="BRAKE" split="true"/>
	</gear>

	<!-- right gear s=3.2 d=0.5 sf=.4 df=.2-->
	<gear x="-1.64" y="-0.879" z="-1.254" compression="0.1" spring="3.5" damp="0.75" sfric=".5" dfric=".4" initial-load="0.8">
		<control-input axis="/controls/gear/brake-right" control="BRAKE" split="true"/>
		<control-input axis="/controls/gear/brake-parking" control="BRAKE" split="true"/>
	</gear>

	<!-- tail gear -->
    <gear x="-3.15" y="0.000" z="-1.029" compression="0.1" spring="1" damp="1.0" sfric="0.8" dfric="0.6" ignored-by-solver="1">
    </gear>


	<tank x="-1.84" y="0.0" z="-.33" capacity="5"/>
	<!-- work around bug in native-fdm.cxx:184 -->
	<tank x="0" y="0" z="0" capacity="0"/>

	<ballast x="-1.12" y="0.0" z="-0.19" mass="225"/>
	<!-- pilot -->

	<ballast x="-1.55" y="-0.25" z=" 0.15" mass="13.33"/>
	<ballast x="-1.55" y=" 0.25" z=" 0.15" mass="13.33"/>
	<ballast x="-1.55" y=" 0.0" z=" 4.0" mass="13.33"/>
	<ballast x="-1.55" y=" 0.0" z="-3.7" mass="13.33"/>
	<ballast x=" 0.85" y=" 0.0" z=" 0.15" mass="13.33"/>
	<ballast x="-3.95" y=" 0.0" z=" 0.15" mass="13.33"/>
	<!-- to get the cg and the inertia tensor correct -->

</airplane>


