    </condition>
  </animation>

  <!-- Manette moteur -->
  <animation>
    <type>rotate</type>
    <object-name>throttle</object-name>
    <property>/controls/engines/engine[0]/throttle</property>
    <factor> 50 </factor>
    <center>
      <x-m> -0.793 </x-m>
      <y-m> -0.240 </y-m>
      <z-m> -0.566 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y> -1.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <!-- Sélection Pâles/blur  rotor parincipal -->
  <animation>
    <type>select</type>
    <object-name>propdisc1</object-name>
    <condition>
      <greater-than>
        <property>rotors/main/rpm</property>
        <value>150</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>propblur1</object-name>
    <condition>
    <and>
      <greater-than>
        <property>rotors/main/rpm</property>
        <value>60</value>
      </greater-than>
      <less-than>
        <property>rotors/main/rpm</property>
        <value>150.001</value>
      </less-than>
    </and>
    </condition>
  </animation>

